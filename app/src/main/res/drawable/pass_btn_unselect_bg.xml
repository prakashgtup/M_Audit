<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="1dp"
        android:bottomLeftRadius="5dp"
        android:bottomRightRadius="0dp"
        android:topRightRadius="0dp"
        android:topLeftRadius="5dp"
        />
    <gradient
        android:angle="270"
        android:centerX="50%"
        android:centerColor="#9BB68A"
        android:startColor="#BFE8A6"
        android:endColor="#9DB88B"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />

    <stroke
        android:width="2dp"
        android:color="#9BB28E"
        />
</shape>